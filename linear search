//linear search
import java.util.*;
public class Main{
    public static void main(String...args){
        int[] arr={3,7,2,9,5};
        int target=6;
        int result=linearSearch(arr,target);
        if(result!=-1){
            System.out.println("the value found at "+ result);
        }
        else{
            System.out.println("the value is not found");
        }
    }
    public static int linearSearch(int[] arr,int target){
        for(int i=0;i<arr.length;i++){
            if(arr[i]==target){
                return i;
            }
        }
        return -1;
    }
}

intput;9

output;
the value found at 3
